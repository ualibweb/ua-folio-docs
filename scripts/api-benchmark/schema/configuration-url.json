{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "api-benchmark Test URL",
  "description": "Configuration for a single URL to test against to run",
  "type": "object",
  "properties": {
    "name": {
      "type": "string"
    },
    "base": {
      "type": "string",
      "examples": ["http://localhost:8084"]
    },
    "headers": {
      "type": "object"
    }
  },
  "required": ["name", "base"],
  "additionalProperties": false
}
