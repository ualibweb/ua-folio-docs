{
  "name": "multi-api-benchmark",
  "version": "1.0.8",
  "description": "A simple tool for testing API speeds",
  "author": "Noah Overcash <ncovercash@ua.edu>",
  "license": "Unlicense",
  "main": "./dist/index.js",
  "bin": "./dist/bin.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p tsconfig.json && copyfiles -V schema/**/*.json dist && copyfiles -V bin.js dist",
    "start": "npx ts-node index.ts --unhandled-rejections=strict",
    "dump-schema": "npx ts-node schema/bundle.ts --unhandled-rejections=strict",
    "test": "",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^9.0.9",
    "ajv": "^8.9.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.10.0",
    "csv-writer": "^1.6.0",
    "debug": "^4.3.3",
    "fast-stats": "^0.0.6",
    "https-proxy-agent": "^5.0.0",
    "js-yaml": "^4.1.0",
    "memoizee": "^0.4.15",
    "merge-options": "^3.0.4",
    "node-fetch": "^2.6.6",
    "supports-color": "^9.2.1",
    "yaml": "^1.10.2",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@types/cli-progress": "^3.9.2",
    "@types/debug": "^4.1.7",
    "@types/fast-stats": "^0.0.32",
    "@types/js-yaml": "^4.0.5",
    "@types/memoizee": "^0.4.7",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^2.5.3",
    "@types/prompt": "^1.1.2",
    "@types/yargs": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.7.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
